{
  "name": "hubapi",
  "version": "1.0.0",
  "main": "./dist/app.js",
  "license": "MIT",
  "scripts": {
    "watch:ts": "tsc --watch",
    "watch:node": "nodemon .",
    "watch": "rm -rf ./dist && tsc && concurrently -k -p \"[{name}]\" -n \"Typecript, node\" -c \"blue.bold, magenta.bold\" \"yarn run watch:ts\" \"yarn run watch:node\"",
    "dok:active": "docker-compose up -d --build mongo-db",
    "dok:deavtivate": "docker-compose down && docker volume prune && docker system prune -a -f",
    "lint": "eslint --ext .ts .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write --loglevel silent './**/*.{json,js}'",
    "initialize": "git clone -b v0.1.0 git@github.com:gonzalesgustavo/Gen10-Generator.git && cd ./Gen10-Generator && yarn install",
    "gen10": "node ./Gen10-Generator/index.js",
    "test": "jest --runInBand --forceExit --detectOpenHandles --colors --verbose --reporters=default --watchAll",
    "test:ci": "jest --ci --runInBand --silent --colors --coverage"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/connect-mongo": "^3.1.3",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.14",
    "@types/mongoose": "^5.10.1",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.14.10",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.1.2",
    "winston": "^3.3.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.10.17",
    "multer": "^1.4.2",
    "nodemon": "^2.0.6",
    "uuid": "^8.3.1"
  }
}
